{
    "manifest_version": 2,
    "name": "news_feed",
    "description": "NEWS",
    "version": "0.1",

    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },


    "permissions": [
        "storage",
        "activeTab",
        "https://ajax.googleapis.com/",
        "https://*/*",
        "http://www.google.com/",
        "https://www.google.com/",
        "http://*.twitter.com/*",
        "http://*.twing.com/*"

    ],
    // "content_security_policy": "script-src 'self' https://cdn.api.twitter.com; object-src 'self'",

    // "content_security_policy": "script-src 'self' https://platform.twitter.com; object-src 'self'",
    // "content_security_policy": "script-src 'self' https://syndication.twimg.com; object-src 'self'"

    // "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"


     // "content_security_policy": "script-src 'self' https://platform.twitter.com https://cdn.api.twitter.com https://syndication.twimg.com; object-src 'self'"


"content_security_policy":  "script-src 'self' https://platform.twitter.com https://cdn.api.twitter.com https://cdn.syndication.twimg.com https://syndication.twimg.com https://syndication.twitter.com; object-src 'self'"
// "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"



}
